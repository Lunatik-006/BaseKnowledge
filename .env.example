TELEGRAM_BOT_TOKEN=
BOT_API_TOKEN=
REPLICATE_API_TOKEN=
MILVUS_URI=http://milvus:19530
# Database config
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
POSTGRES_DB=baseknowledge

# Optional override (if you prefer a single URI variable):
# POSTGRES_URI=postgresql+psycopg://postgres:postgres@postgres:5432/baseknowledge
VAULT_DIR=/tmp/vault
PUBLIC_URL=
# Tip: for initial setup, use http://your-domain until certificates are issued
LOG_LEVEL=INFO
# LLM diagnostics & limits
# Set to 'true' to log raw Replicate request/response payloads at INFO level
LLM_LOG_PAYLOADS=false
# Increase if you observe empty responses from gpt-5-structured
LLM_MAX_OUTPUT_TOKENS=2048
# Token cap for gpt-5-nano style models
LLM_MAX_COMPLETION_TOKENS=1024
EMBEDDINGS_MODEL=nomic-ai/nomic-embed-text-v1.5
EMBEDDING_DIM=768
